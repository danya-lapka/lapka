    "build": "tsup"
  },
  "keywords": [
    "vite",
    "plugin",
    "css",
    "atomic"
  ],
  "author": "Danya Lapka",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  }
}