{
  "name": "@danya-lapka/css-vite",
  "version": "1.0.11",
  "private": false,
  "description": "Custom atomic CSS generator for Vite",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./empty.css": "./empty.css"
  },
  "files": [
    "dist",
    "empty.css"
  ],
  "scripts": {
    "build": "tsup"
  },
  "keywords": [
    "vite",
    "plugin",
    "css",
    "atomic"
  ],
  "author": "Danya Lapka",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.4"
  },
  "peerDependencies": {
    "vite": "^7.2.4"
  }
}