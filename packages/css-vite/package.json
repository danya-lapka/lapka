{
  "name": "@danya-lapka/css-vite",
  "version": "1.0.0",
  "private": false,
  "description": "Custom atomic CSS generator for Vite",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
